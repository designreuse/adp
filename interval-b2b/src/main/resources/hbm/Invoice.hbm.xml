<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.interval.dao.models.Invoice" table="INVOICE" schema="PUBLIC" catalog="PUBLIC">
        <id name="id">
            <column name="ID" sql-type="integer" not-null="true"/>
        </id>
        <property name="orderId">
            <column name="ORDER_ID" sql-type="integer" not-null="true"/>
        </property>
        <property name="userId">
            <column name="USER_ID" sql-type="integer" not-null="true"/>
        </property>
        <property name="createdTime">
            <column name="CREATED_TIME" sql-type="date"/>
        </property>
        <property name="updatedTime">
            <column name="UPDATED_TIME" sql-type="date"/>
        </property>
        <many-to-one name="orderDetailByOrderId" class="com.interval.dao.models.OrderDetail" insert="false" update="false">
            <column name="ORDER_ID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>