<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.interval.dao.models.Vendor" table="VENDOR" schema="PUBLIC" catalog="PUBLIC">
        <id name="id">
            <column name="ID" sql-type="integer" not-null="true"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="character" length="60"/>
        </property>
        <property name="address1">
            <column name="ADDRESS_1" sql-type="character" length="60"/>
        </property>
        <property name="address2">
            <column name="ADDRESS_2" sql-type="character" length="60"/>
        </property>
        <property name="city">
            <column name="CITY" sql-type="character" length="60"/>
        </property>
        <property name="state">
            <column name="STATE" sql-type="character" length="60"/>
        </property>
        <property name="zip">
            <column name="ZIP" sql-type="character" length="60"/>
        </property>
        <property name="country">
            <column name="COUNTRY" sql-type="character" length="60"/>
        </property>
        <property name="phone">
            <column name="PHONE" sql-type="character" length="60"/>
        </property>
        <property name="email">
            <column name="EMAIL" sql-type="character" length="60"/>
        </property>
        <property name="createdTime">
            <column name="CREATED_TIME" sql-type="date"/>
        </property>
        <property name="updatedTime">
            <column name="UPDATED_TIME" sql-type="date"/>
        </property>
        <set name="vendorCentersById" inverse="true">
            <key>
                <column name="VENDOR_ID" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.interval.dao.models.VendorCenter"/>
        </set>
    </class>
</hibernate-mapping>