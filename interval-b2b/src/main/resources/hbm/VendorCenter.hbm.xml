<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.interval.dao.models.VendorCenter" table="VENDOR_CENTER" schema="PUBLIC" catalog="PUBLIC">
        <id name="id">
            <column name="ID" sql-type="integer" not-null="true"/>
        </id>
        <property name="centerId">
            <column name="CENTER_ID" sql-type="integer" not-null="true"/>
        </property>
        <property name="vendorId">
            <column name="VENDOR_ID" sql-type="integer" not-null="true"/>
        </property>
        <many-to-one name="centerByCenterId" class="com.interval.dao.models.Center" insert="false" update="false">
            <column name="CENTER_ID" not-null="true"/>
        </many-to-one>
        <many-to-one name="vendorByVendorId" class="com.interval.dao.models.Vendor" insert="false" update="false">
            <column name="VENDOR_ID" not-null="true"/>
        </many-to-one>
        <set name="productsById" inverse="true">
            <key>
                <column name="VENDOR_CENTER_ID" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.interval.dao.models.Product"/>
        </set>
    </class>
</hibernate-mapping>